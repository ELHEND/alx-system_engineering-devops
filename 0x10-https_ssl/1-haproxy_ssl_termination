global
    ssl-engine on
    ssl-ca /etc/letsencrypt/live/holberton.online.com/fullchain.pem
    ssl-cert /etc/letsencrypt/live/holberton.online.com/cert.pem
    ssl-key /etc/letsencrypt/live/holberton.online.com/privkey.pem

defaults
    mode http
    option httplog
    option dontlognull
    option http-server-close

frontend www
    bind *:443
    mode http
    default_backend www_backend

backend www_backend
    balance roundrobin
    server web1 100.26.173.246:80
    server web2 54.210.234.144:80
